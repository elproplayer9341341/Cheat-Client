RunBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
RunBox.BorderSizePixel = 0
RunBox.Position = UDim2.new(0.0226337444, 0, 0.841509461, 0)
RunBox.Size = UDim2.new(0, 165, 0, 34)
RunBox.Font = Enum.Font.FredokaOne
RunBox.Text = "Run box"
RunBox.TextColor3 = Color3.fromRGB(255, 255, 255)
