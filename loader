local SettingsFrame = Instance.new("Frame")
local UICorner_4 = Instance.new("UICorner")
local Settings = Instance.new("TextLabel")
local Close = Instance.new("TextLabel")
local Close_2 = Instance.new("TextButton")
local UICorner_5 = Instance.new("UICorner")

CheatClient.Name = "CheatClient"
CheatClient.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
CheatClient.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Home.Name = "Home"
Home.Parent = CheatClient
Home.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
Home.BorderColor3 = Color3.fromRGB(0, 0, 0)
Home.BorderSizePixel = 0
Home.Position = UDim2.new(0.136764705, 0, 0.306397319, 0)
Home.Size = UDim2.new(0, 486, 0, 265)

TextLabel.Parent = Home
TextLabel.BackgroundColor3 = Color3.fromRGB(0, 1, 0)
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Size = UDim2.new(0, 486, 0, 28)
TextLabel.Font = Enum.Font.FredokaOne
