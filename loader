Close.TextWrapped = true

Close_2.Name = "Close"
Close_2.Parent = SettingsFrame
Close_2.BackgroundColor3 = Color3.fromRGB(238, 91, 65)
Close_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
Close_2.BorderSizePixel = 0
Close_2.Position = UDim2.new(0.651092172, 0, 0.184366524, 0)
Close_2.Size = UDim2.new(0, 81, 0, 32)
Close_2.Font = Enum.Font.FredokaOne
Close_2.Text = "Run CMD"
Close_2.TextColor3 = Color3.fromRGB(255, 255, 255)
Close_2.TextScaled = true
Close_2.TextSize = 14.000
Close_2.TextWrapped = true

UICorner_5.CornerRadius = UDim.new(0, 5)
UICorner_5.Parent = Close_2

Home.Active = true
Home.Draggable = true
SettingsFrame.Visible = true
SettingsFrame.Transparency = 1
Close.TextTransparency = 1
Close_2.BackgroundTransparency = 1
Settings.TextTransparency = 1
Settings.Visible = false
Close_2.Visible = false

Close_2.MouseButton1Click:Connect(function()
	game:shutdown()
end)

RunBox.MouseButton1Click:Connect(function()
	pcall(Scriptbox.Text)
end)

Clearbox.MouseButton1Click:Connect(function()
	Scriptbox.Text = ""
end)

local active = false
SettingsButton.MouseButton1Click:Connect(function()
	if not active then
	active = true
	local TweenService = game:GetService("TweenService")
	local T1 = TweenService:Create(SettingsFrame, TweenInfo.new(1), {BackgroundTransparency = 0})
	local T2 = TweenService:Create(Close, TweenInfo.new(1), {TextTransparency = 0})
	local T3 = TweenService:Create(Close_2, TweenInfo.new(1), {BackgroundTransparency = 0})
	local T4 = TweenService:Create(Settings, TweenInfo.new(1), {TextTransparency = 0})
		
		Settings.Visible = true
		Close_2.Visible = true
		T1:Play()
		T2:Play()
		T3:Play()
		T4:Play()
		
	else
	if active then
		active = false
			local TweenService = game:GetService("TweenService")
			local T1 = TweenService:Create(SettingsFrame, TweenInfo.new(1), {BackgroundTransparency = 1})
			local T2 = TweenService:Create(Close, TweenInfo.new(1), {TextTransparency = 1})
			local T3 = TweenService:Create(Close_2, TweenInfo.new(1), {BackgroundTransparency = 1})
			local T4 = TweenService:Create(Settings, TweenInfo.new(1), {TextTransparency = 1})

			T1:Play()
			T2:Play()
			T3:Play()
			T4:Play()
			
			T3.Completed:Connect(function()
				Close_2.Visible = false
			end)
			
			T4.Completed:Connect(function()
				Settings.Visible = false
			end)
			
	end
	end
end)
end)


