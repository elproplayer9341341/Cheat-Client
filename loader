SettingsButton.BackgroundTransparency = 1.000
SettingsButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
SettingsButton.BorderSizePixel = 0
SettingsButton.Position = UDim2.new(0.919753075, 0, 0.864150941, 0)
SettingsButton.Size = UDim2.new(0, 39, 0, 36)
SettingsButton.Image = "rbxassetid://2790678226"

SettingsFrame.Name = "SettingsFrame"
SettingsFrame.Parent = Home
SettingsFrame.BackgroundColor3 = Color3.fromRGB(32, 31, 32)
SettingsFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
SettingsFrame.BorderSizePixel = 0
